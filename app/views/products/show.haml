- content_for :javascript do
  :coffee
    UIDemo.app.on 'application:initialize', (world) ->
      world.productCategories.reset #{product_categories_json}
      activeCategory = world.productCategories.get(#{@product_category.id})
      activeCategory.products.reset #{product_category_products_json}
      activeProduct = activeCategory.products.get(#{@product.id})
      activeProduct.set #{product_details_json}

